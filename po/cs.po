#: src/reuse/cli/annotate.py:426
msgid "PATH"
msgstr "CESTA"

#: src/reuse/cli/annotate.py:476
#, python-brace-format
msgid "'{path}' is a binary, therefore using '{new_path}' for the header"
msgstr "'{path}' je binární soubor, proto použijte '{new_path}' pro hlavičku"

#: src/reuse/cli/common.py:58
#, python-brace-format
msgid ""
"'{path}' could not be parsed. We received the following error message: "
"{message}"
msgstr ""
"'{path}' se nepodařilo zpracovat. Obdrželi jsme následující chybové hlášení: "
"{message}"

